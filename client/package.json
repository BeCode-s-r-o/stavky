  "scripts": {
    "start:forex": "REACT_APP_WEBSITE_TYPE=forex react-scripts start",
    "start:stavkove": "REACT_APP_WEBSITE_TYPE=stavkove react-scripts start",
    "start": "react-scripts --openssl-legacy-provider start",
    "build:forex": "REACT_APP_WEBSITE_TYPE=forex react-scripts build",
    "build:stavkove": "REACT_APP_WEBSITE_TYPE=stavkove react-scripts build",
    "imageoptim": "imageoptim:publicÂ && imageoptim:assets",
    "imageoptim:public": "yarn imageoptim:jpg:public && yarn imageoptim:png:public",
    "imageoptim:assets": "yarn imageoptim:jpg:assets && yarn imageoptim:png:assets",
    "imageoptim:jpg:assets": "imageoptim \"src/assets/**/*.jpg\"",
    "imageoptim:png:assets": "imageoptim  \"src/assets/**/*.png\"",
    "imageoptim:jpg:public": "imageoptim \"public/**/*.jpg\"",
    "imageoptim:png:public": "imageoptim  \"public/**/*.png\"",
    "copy:forex": "cp -r build forex",
    "copy:stavkove": "cp -r build stavkove",
    "build-and-copy:forex": "npm run build:forex && npm run copy:forex",
    "build-and-copy:stavkove": "npm run build:stavkove && npm run copy:stavkove"
  },
